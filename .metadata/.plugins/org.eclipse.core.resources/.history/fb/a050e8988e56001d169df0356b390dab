package com.techno.employeemanagement.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.techno.employeemanagement.dao.EmployeeDao;
import com.techno.employeemanagement.dao.EmployeeDaoSec;
import com.techno.employeemanagement.entity.EmployeePrimaryInfo;
import com.techno.employeemanagement.entity.EmployeeSecondaryInfo;

@Service
public class EmployeeServiceImpl implements EmployeeSevice {

	@Autowired
	private EmployeeDao dao;
	@Autowired
	private EmployeeDaoSec daoSec;

	@Override
	public EmployeePrimaryInfo getRegister(EmployeePrimaryInfo employeePrimaryInfo, EmployeeSecondaryInfo employeeSecondaryInfo) {
		EmployeePrimaryInfo primaryInfo = dao.save(employeePrimaryInfo);
		dao.save(employeeSecondaryInfo);
		return null;
	}

}
